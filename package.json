{
  "name": "redux-ddd",
  "version": "1.0.1",
  "description": "Redux extension inspired by Domain Driven Design principles",
  "main": "src/redux-ddd.js",
  "files": [
    "src",
    "lib",
    "es",
    "dist"
  ],
  "scripts": {
    "clean": "rimraf lib dist es",
    "build": "npm run build:commonjs && npm run build:es",
    "prepublish": "npm run clean && npm run test && npm run build",
    "posttest": "npm run lint",
    "lint": "eslint src test",
    "test": "jasmine",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alessiodm/redux-ddd.git"
  },
  "keywords": [
    "redux",
    "side-effects",
    "ddd",
    "action-listener",
    "connect"
  ],
  "author": "Alessio Della Motta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alessiodm/redux-ddd/issues"
  },
  "homepage": "https://github.com/alessiodm/redux-ddd#readme",
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-register": "^6.22.0",
    "cross-env": "^3.1.4",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "jasmine": "^2.5.3",
    "redux": "^3.6.0",
    "rimraf": "^2.5.4"
  },
  "dependencies": {}
}
